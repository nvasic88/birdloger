{
    "name": "biologer/birdloger",
    "type": "project",
    "description": "Database of bird species in Serbia",
    "keywords": ["biologer", "birdloger", "birds", "serbia"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nenad Živanović",
            "email": "nen.zivanovic@gmail.com",
            "homepage": "https://nenadzivanovic.in.rs",
            "role": "Developer"
        },
        {
            "name": "Nikola Vasić",
            "email": "nikolavasic88@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "astrotomic/laravel-translatable": "^11.9.1",
        "box/spout": "^2.7.3",
        "doctrine/dbal": "^2.13.1",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^1.0.6",
        "guzzlehttp/guzzle": "^6.5.5",
        "intervention/image": "^2.5.1",
        "laravel/framework": "^7.30.4",
        "laravel/passport": "^9.4.0",
        "laravel/tinker": "^2.6.1",
        "laravel/ui": "^2.5.0",
        "maxakawizard/json-collection-parser": "^1.8.0",
        "mcamara/laravel-localization": "^1.6.1",
        "meyfa/php-svg": "^0.11.0",
        "myclabs/php-enum": "^1.8.0",
        "nikazooz/laravel-captcha": "^1.3.0",
        "php-coord/php-coord": "^3.1.2",
        "runalyze/dem-reader": "^1.1",
        "spatie/laravel-activitylog": "^3.17.0",
        "spatie/laravel-backup": "^6.16.0",
        "spatie/laravel-cookie-consent": "^2.12.13",
        "spatie/laravel-menu": "^3.7.1",
        "stevebauman/purify": "^v3.0.3",
        "tightenco/ziggy": "^0.9.4"
    },
    "require-dev": {
        "beyondcode/laravel-dump-server": "^1.7",
        "facade/ignition": "^2.9.0",
        "itsgoingd/clockwork": "^5.0",
        "mockery/mockery": "^1.4.3",
        "nunomaduro/collision": "^v5.4.0",
        "phpunit/phpunit": "^9.5.4",
        "wnx/laravel-stats": "^v2.5.2"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "test": "phpunit"
    }
}
